{
  "manifest_version": 2,
  "short_name": "Easy App-ly",
  "name": "Easy App-ly",
  "version": "1.0.1",
  "description": "Easy App-ly web extension for easier job applications.",
  "icons": {
    "192":"logo192.png",
    "512":"logo512.png",
    "16": "logo16.png",
    "24": "favicon.ico",
    "32": "favicon.ico",
    "64": "favicon.ico",
    "48": "logo48.png",
    "128": "logo128.png"

  },
  "browser_action": {
    "default_icon": "logo48.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ] 
}
